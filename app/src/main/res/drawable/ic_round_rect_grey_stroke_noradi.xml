<layer-list xmlns:android="http://schemas.android.com/apk/res/android">


    <item >
        <shape android:shape="rectangle">
            <stroke android:width="1dp" android:color="#d2d6db"></stroke>
            <solid android:color="#ffff" />
            <corners android:radius="0dp"></corners>
            <item android:name="android:shadowColor">#f3f5f7</item>
            <!-- TextView shadow offsets do not respect scale. -->
            <item android:name="android:shadowDx">0</item>
            <item android:name="android:shadowDy">3</item>
        </shape>
    </item>

</layer-list>